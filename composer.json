{
    "name": "mjaysidor/papi-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "Skeleton to create new papi API application",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "ext-pgsql": "*",
        "workerman/workerman": "^4.0",
        "nikic/fast-route": "^1.3",
        "ext-readline": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "papi\\": "papi/",
            "config\\": "config/",
            "migrations\\": "migrations/"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^5.2",
        "symfony/yaml": "^5.2",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "phpstan/phpstan": "0.12.x-dev"
    }
}
